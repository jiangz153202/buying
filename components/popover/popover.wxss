/* components/popover/popover.wxss */
page{
  --border-radius:12rpx;
  --border-radius2:24rpx;
}

@keyframes mask{
  from{
    opacity: 0;
    visibility: hidden;
  }
  to{
    opacity: 1;
    visibility: visible;
  }
}

@keyframes mt{
  0%{
    opacity: 0;
    visibility: hidden;
    transform: translateX(-50%) scale(0);
  }
  75%{
    transform: translateX(-50%) scale(1.05);
  }
  100%{
    opacity: 1;
    transform: translateX(-50%) scale(1);
    visibility: visible;
  }
}

.mask{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  right: 0;
  background-color: rgba(0, 0, 0, .5);
  z-index: 100;
  width: 100vw;
  height: 100vh;
}


.mask~.close,.container,.mask{
  visibility: hidden;
  
}

.mask.active,.close.active{
  animation: .8s ease-in-out 0s mask forwards;
}
.container.active{
  animation: .8s ease-in-out 0s mt forwards;
}




.container{
  position: fixed;
  top: 172rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 548rpx;
  z-index: 101;
 
}



.mask~.close{
  
  background: no-repeat url("data:image/png;base64,");
  background-size: cover;
  position: fixed;
  z-index: 101;
  right: 48rpx;
  top: 48rpx;
  width: 44rpx;
  height: 44rpx;
 
}

.container .banner{
  width: 548rpx;
  display: block;
  border-radius: var(--border-radius);
}

.container .footer{
  width: 100%;
  background:no-repeat url("data:image/png;base64,");
  box-sizing: border-box;
  padding: 20rpx 68rpx;
  margin-top: 24rpx;
  border-radius: var(--border-radius);
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.container .footer::before{
  content: "";
  background: no-repeat url("data:image/png;base64,");
  background-size: 100%;
  height: 74rpx;
  position: absolute;
  width: 100%;
  top: -54rpx;
  left: 0;
  right: 0;
}

.container .footer .btn{
  width: 172rpx;
  height: 52rpx;
  line-height: 52rpx;
  border-radius: var(--border-radius2);
  font-size: 30rpx;
  font-family:Arial, Helvetica, sans-serif;
  color: #fff;
  text-align: center;
}

.container .footer .btn.btn-share{
  background-color: #458f51;
}

.container .footer .btn.btn-buy{
  background-color: rgb(216,161,78);
}
